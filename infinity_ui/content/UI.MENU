function rgRadarUpdateHotkeySlotPosition(close)

	if RgRadarShowPcEffects == 1 then
		local screenWidth, screenHeight = Infinity_GetScreenSize()
		--
		local count = {0,0,0,0,0,0 }
		--local nosel = 0
