		end
	elseif rgGameEngine == 3 then
		if startEngine:GetCampaign() == 1 then
			if rgCurrentCampaign == 7 then
				return 7
			else
				return 6
			end
		end
	elseif rgGameEngine == 4 then
		return rgCampaigns[currentCampaign].bigLogo
	end
