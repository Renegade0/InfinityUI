function rgRadarInspectSprite()
	if SPRITE_ROW ~= nil then
		for s=1,#my_radar_sprites do
			local candidate = my_radar_sprites[s]
			if candidate.m_id == rgRadarSelectedCreId then
				SPRITE_ROW = s
			end
		end
		my_screen_radar_Inspect_sprite(SPRITE_ROW)
	end
end


