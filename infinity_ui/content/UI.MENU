function rgChargenSteps(t)
	local target = t
	if t == 9 then --skills
		target = 8
	end
	for index = 1, 10, 1 do
		if createCharScreen:GetCurrentStep() == target then 
			break 
		else
			createCharScreen:OnMainBackButtonClick()
		end
	end
	if t == 9 then --skills
		createCharScreen:OnMenuButtonClick()
		createCharScreen:OnDoneButtonClick()
		createCharScreen:OnMenuButtonClick()
	end
end
