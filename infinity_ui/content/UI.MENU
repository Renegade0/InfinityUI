	end
	
	return ret
end

function rgBestApr(text)
	if text == 6 then
		return '1/2'
	elseif  text == 7 then
		return '3/2'
	elseif  text == 8 then
		return '5/2'
	elseif  text == 9 then
		return '7/2'
	elseif  text == 10 then
		return '9/2'
	else
		return text
	end
end

