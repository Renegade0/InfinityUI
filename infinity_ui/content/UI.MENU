function rgSetZoomMainMenuArea(element,x,y,a,b)
	w, h = Infinity_GetScreenSize()
	
	local currH = 768
	local currW = w / (h / currH)
	local n = 1
	local difx = (currW - (currW*n)) / 2
	local dify = (currH - (currH*n)) / 2
	
	
	Infinity_SetArea(element, difx + (x*n), dify + (y*n), a*n, b*n)
	
end

