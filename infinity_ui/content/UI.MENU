		if rgGameEngine == 4 then
			if rgCampaigns[buttonNum] ~= nil then
				startEngine:OnCampaignButtonClick(rgCampaigns[buttonNum].id,true)
				Infinity_SetINIValue('Program Options','Active Campaign',buttonNum)
			else
